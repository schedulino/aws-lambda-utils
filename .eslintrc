{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "plugins": [
        "babel"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "generator-star-spacing": 0,
        "babel/generator-star-spacing": 1,
        "indent": [2, 4],
        "comma-dangle": [2, "never"]
    }
}
