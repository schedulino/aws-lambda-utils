{
  "name": "schedulino-utils",
  "version": "0.0.1",
  "description": "Schedulino project utilities.",
  "license": "GPL-3.0",
  "private": true,
  "author": {
    "email": "support@schedulino.com",
    "name": "Schedulino Team",
    "url": "https://schedulino.com"
  },
  "maintainers": [
    {
      "name": "Schedulino Team",
      "email": "support@schedulino.com",
      "url": "https://schedulino.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schedulino/schedulino-utils.git"
  },
  "bugs": {
    "url": "https://github.com/schedulino/schedulino-utils/issues"
  },
  "homepage": "https://github.com/schedulino/schedulino-utils#readme",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint lib/**/*.js",
    "build": "rimraf dist/* && npm run lint && babel lib --out-dir dist",
    "audit": "nsp check --output summary",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap",
    "pretest": "npm run lint",
    "postmerge": "npm install",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "audit"
  ],
  "dependencies": {
    "joi": "10.4.2",
    "boom": "4.3.1",
    "@schedulino/lambda-logger": "0.0.11"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-loader": "1.7.1",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "2.2.0",
    "nsp": "2.6.3",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.1",
    "webpack": "2.5.1"
  },
  "engines": {
    "node": ">=6.10.0",
    "npm": ">=3.0.0"
  }
}
