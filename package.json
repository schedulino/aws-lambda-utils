{
  "name": "schedulino-utils",
  "version": "0.0.1",
  "description": "Schedulino project utilities.",
  "license": "GPL-3.0",
  "private": true,
  "author": {
    "email": "support@schedulino.com",
    "name": "Schedulino Team",
    "url": "https://schedulino.com"
  },
  "maintainers": [
    {
      "name": "Schedulino Team",
      "email": "support@schedulino.com",
      "url": "https://schedulino.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schedulino/schedulino-utils"
  },
  "bugs": {
    "url": "https://github.com/schedulino/schedulino-utils"
  },
  "homepage": "https://github.com/schedulino/schedulino-utils#readme",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "lint": "eslint **/*.js",
    "audit": "nsp check --output summary",
    "missing": "npm ls --depth 1",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap",
    "pretest": "npm run lint",
    "postmerge": "npm install",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "audit",
    "missing"
  ],
  "dependencies": {
    "aws-sdk": "2.6.9"
  },
  "devDependencies": {
    "nsp": "2.6.2",
    "pre-commit": "1.1.3",
    "eslint": "3.8.1",
    "eslint-loader": "1.6.0",
    "babel-eslint": "7.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-config-airbnb-base": "9.0.0",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-preset-stage-2": "6.17.0"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0"
  }
}
